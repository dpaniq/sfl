{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/.*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache",
    "Access-Control-Allow-Origin": "https://sfl-backend.azurewebsites.net",
    "Access-Control-Allow-Methods": "POST, GET, OPTIONS, PATCH, PUT, DELETE"
  }
}
