{
  "routes": [
    {
      "route": "/",
      "rewrite": "/dist/index.html",
      "statusCode": 200
    },
    {
      "route": "/assets/*",
      "rewrite": "/assets/*",
      "statusCode": 404
    },
    {
      "route": "/.*",
      "rewrite": "/dist/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/dist/index.html"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  }
}
